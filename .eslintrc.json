{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "eslint:recommended",
        // "plugin:@typescript-eslint/recommended",
        "plugin:import/recommended"
    ],
    "overrides": [],
    // "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        // "@typescript-eslint",
        "import"
    ],
    "settings": {
        "import/extensions": [
            ".js",
            ".jsx"
        ]
    },
    "rules": {
        // "import/no-restricted-paths": [
        //     2,
        //     {
        //         "zones": [
        //             {
        //                 "target": "./debug",
        //                 "from": "./src"
        //             }
        //         ]
        //     }
        // ],
        "import/no-deprecated": "off",
        "import/namespace": "error",
        // "import/no-unused-modules": [
        //     2,
        //     {
        //         "unusedExports": true
        //     }
        // ],
        "import/no-named-as-default": "error",
        "import/named": "off",
        "import/no-cycle":"error"
    }
}