{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:import/recommended"
    ],
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "import"
    ],
    "settings": {
        "import/extensions": [
            ".js",
            ".jsx"
        ]
    },
    "rules": {
        "import/named": "error"
    }
}